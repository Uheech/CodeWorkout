def solution(a, b):
# 최대공약수를 먼저 찾아서 하나의 리스트에 집어넣기
    num_list=[]
    for i in range(1,b+1):
        if (a%i==0) and (b%i==0):
            num_list.append(i)
            
# 기약분수로 만들어주기> 분자/분모를 최대공약수로 나눠주기
    num_list.remove(1)
    for n in num_list:
        if a%n==0 and b%n==0:
            a=a//n
            b=b//n
# 기약분수일 때
    if b%2==0 or b%5==0:
        return 1
    return 2
